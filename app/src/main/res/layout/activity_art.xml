<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_art"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.art.art.ART">

    <TextView
        android:id="@+id/hello"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!" />
    <Button
        android:id="@+id/assistance"
        android:layout_width="200px"
        android:layout_height="50px"
        android:layout_below="@+id/hello"
        android:text="Assist"
        android:onClick="load_assist_activity"/>
    <Button
        android:id="@+id/repair"
        android:layout_width="200px"
        android:layout_height="50px"
        android:layout_below="@+id/assistance"
        android:text="Repair"
        android:onClick="load_repair_activity"/>
    <Button
        android:layout_width="200px"
        android:layout_height="50px"
        android:layout_below="@+id/repair"
        android:text="Transportation"
        android:onClick="load_transportation_activity"/>

    <TextView
        android:id="@+id/log"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />
</RelativeLayout>
